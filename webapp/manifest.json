{
  "_version": "1.60.0",
  "sap.app": {
    "id": "com.kaar.quality",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.0",
      "toolsId": "b32169cf-850e-4a05-a835-6859d1330cc3"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZBL_QUA_LOGIN_VIEW_CDS/",
        "type": "OData",
        "settings": {
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "inspectionService": {
        "uri": "/sap/opu/odata/sap/ZBL_QUA_INSPECTION_VIEW_CDS/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.120.14",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.kaar.quality.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "useBatch": false
        }
      },
      "inspection": {
        "dataSource": "inspectionService",
        "settings": {
          "useBatch": false
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.kaar.quality.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "login",
          "pattern": "",
          "target": [
            "login"
          ]
        },
        {
          "name": "dashboard",
          "pattern": "dashboard",
          "target": [
            "dashboard"
          ]
        },
        {
          "name": "inspectionLots",
          "pattern": "inspectionLots",
          "target": [
            "inspectionLots"
          ]
        },
        {
          "name": "lotDetail",
          "pattern": "lotDetail/{lotId}",
          "target": [
            "lotDetail"
          ]
        }
      ],
      "targets": {
        "login": {
          "viewId": "login",
          "viewName": "Login"
        },
        "dashboard": {
          "viewId": "dashboard",
          "viewName": "Dashboard"
        },
        "inspectionLots": {
          "viewId": "inspectionLots",
          "viewName": "InspectionLotList"
        },
        "lotDetail": {
          "viewId": "lotDetail",
          "viewName": "LotDetail"
        }
      }
    },
    "rootView": {
      "viewName": "com.kaar.quality.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}